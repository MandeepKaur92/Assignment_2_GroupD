<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetDataServlet</servlet-name>
  	<display-name>GetDataServlet</display-name>
  	<description></description>
  	<servlet-class>com.Servlet.GetDataServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AreaServlet</servlet-name>
  	<display-name>AreaServlet</display-name>
  	<description></description>
  	<servlet-class>com.AreaServlet.AreaServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AreaServlet1</servlet-name>
  	<display-name>AreaServlet1</display-name>
  	<description></description>
  	<servlet-class>com.AreaServlet.AreaServlet1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetDataServlet</servlet-name>
  	<url-pattern>/GetDataServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AreaServlet</servlet-name>
  	<url-pattern>/AreaServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AreaServlet1</servlet-name>
  	<url-pattern>/AreaServlet1</url-pattern>
  </servlet-mapping>
</web-app>
